{
  "name": "webpack-marionette",
  "description": "A small boilerplate introducing webpack and es6 features to a Marionette/Backbone application",
  "author": "alexpsi",
  "version": "0.4.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexpsi/webpack-marionette"
  },
  "scripts": {
